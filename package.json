{
  "name": "exif-datify",
  "description": "CLI to rename files with date and time information from Exif data",
  "version": "0.0.0",
  "author": "Harold Waterkeyn",
  "license": "MIT",
  "repository": "hwaterke/exif-datify",
  "homepage": "https://github.com/hwaterke/exif-datify",
  "bugs": "https://github.com/hwaterke/exif-datify/issues",
  "bin": {
    "exif-datify": "./bin/run"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "main": "lib/index.js",
  "oclif": {
    "bin": "exif-datify"
  },
  "types": "lib/index.d.ts",
  "scripts": {
    "format": "prettier --write .",
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.3",
    "chalk": "^4.1.2",
    "luxon": "^2.0.2",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/luxon": "^2.0.3",
    "@types/node": "^12.20.24",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "prettier": "^2.4.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  }
}
