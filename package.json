{
  "name": "exif-datify",
  "description": "CLI to rename files with date and time information from Exif data",
  "version": "0.0.9",
  "author": "Harold Waterkeyn",
  "license": "MIT",
  "repository": "hwaterke/exif-datify",
  "homepage": "https://github.com/hwaterke/exif-datify",
  "bugs": "https://github.com/hwaterke/exif-datify/issues",
  "bin": {
    "exif-datify": "./bin/run"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "main": "dist/index.js",
  "oclif": {
    "bin": "exif-datify",
    "dirname": "exif-datify",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "format": "prettier --write .",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "chalk": "^4",
    "luxon": "^3"
  },
  "devDependencies": {
    "@types/luxon": "^3",
    "@types/node": "^16",
    "@typescript-eslint/eslint-plugin": "^6",
    "@typescript-eslint/parser": "^6",
    "eslint": "^7",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1",
    "eslint-config-prettier": "^8",
    "globby": "^11",
    "oclif": "^3",
    "prettier": "^3.0.0",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6"
  }
}
